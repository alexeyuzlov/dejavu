{
  "name": "sample",
  "version": "0.2.0",
  "type": "module",
  "scripts": {
    "build": "vite build",
    "build:prod": "vite build --base=/dejavu/",
    "dev": "vite",
    "format": "prettier . --write",
    "format:check": "prettier . --check",
    "lint": "eslint \"src/**/*.ts\"",
    "preview": "vite preview",
    "prepare": "husky",
    "test:e2e": "PLAYWRIGHT_BROWSERS_PATH=0 playwright test",
    "test:e2e:install": "NODE_TLS_REJECT_UNAUTHORIZED=0 PLAYWRIGHT_BROWSERS_PATH=0 playwright install",
    "test:e2e:docker": "docker build -t e2e -f Dockerfile.playwright . && docker run --rm e2e"
  },
  "lint-staged": {
    "*.{js,ts,json,yml,yaml,md,html}": "prettier --write"
  },
  "devDependencies": {
    "@playwright/test": "1.58.1",
    "@types/node": "25.2.1",
    "@typescript-eslint/eslint-plugin": "8.54.0",
    "@typescript-eslint/parser": "8.54.0",
    "eslint": "9.39.2",
    "husky": "9.1.7",
    "lint-staged": "16.2.7",
    "prettier": "3.8.1",
    "typescript": "5.9.3",
    "vite": "6.4.1",
    "vite-plugin-static-copy": "3.2.0"
  },
  "dependencies": {
    "phaser": "2.6.2"
  }
}
