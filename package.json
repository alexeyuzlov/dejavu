{
  "name": "sample",
  "version": "0.2.0",
  "scripts": {
    "vite:tsc": "tsc -p tsconfig.vite.json",
    "vite:tsc:watch": "tsc -p tsconfig.vite.json --watch",
    "vite:dev": "npm run vite:tsc:watch",
    "vite:build": "npm run clean && npm run vite:tsc && vite build",
    "vite:serve": "vite",
    "vite:serve:open": "vite --open",
    "vite:preview": "vite preview",
    "vite:preview:open": "vite preview --open",
    "clean": "rm -rf dist",
    "assets:check": "node scripts/check-assets.js"
  },
  "devDependencies": {
    "typescript": "5.9.3",
    "vite": "6.4.1"
  },
  "dependencies": {
    "phaser": "2.1.0"
  }
}
