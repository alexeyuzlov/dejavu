{
  "compilerOptions": {
    "target": "es5",
    "module": "esnext",
    "moduleResolution": "node",
    "sourceMap": true,
    "rootDir": "app/scripts",
    "skipLibCheck": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noEmit": true
  },
  "include": [
    "app/scripts/**/*.ts",
    "node_modules/phaser/typescript/phaser.d.ts"
  ]
}
